        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AplikacijaZaPoslovneKnjige"
        mc:Ignorable="d"
        Title="Izmena konta u kontnom okviru" Height="445.504" Width="354.381" WindowStartupLocation="CenterScreen" Loaded="Window_Loaded">
    <Grid>
        <TextBlock x:Name="tblIzmenaKontaNaslov" HorizontalAlignment="Left" Margin="21,29,0,0" TextWrapping="Wrap" Text="Izmena konta u kontnom okviru" VerticalAlignment="Top" Height="33" Width="311" FontSize="20" FontWeight="Bold" Foreground="#FF212AEA"/>
